{
    "env": {
        "node": true,
        "es6": true,
        "commonjs": true,
        "browser": true
    },
    "extends": "google",
    "rules": {
        "comma-dangle": ["error", "never"],
        "object-curly-spacing": [2, "always"],
        "max-len": [1, 120],
        "no-invalid-this": "off"
    }
}
